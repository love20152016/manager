<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.subject.admin.dao.user.UserInfoMapper">

  <!--查询一个用户信息-->
  <select id="queryUserInfoBYAccount"  parameterType="java.lang.String" resultType="com.subject.admin.dto.user.UserInfoDTO">
        SELECT
            *
        FROM
            sys_user
        where  1 = 1
           and account = #{account}
  </select>
</mapper>